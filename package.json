    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.9",
